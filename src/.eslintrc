{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:react/recommended",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "eslint-plugin-react"
  ],
  "rules": {
    "@typescript-eslint/no-explicit-any": [
      "off"
    ],
    "@typescript-eslint/explicit-function-return-type": [
      "error"
    ]
  }
}